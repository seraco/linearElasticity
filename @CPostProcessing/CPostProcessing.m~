classdef CPostProcessing
    %CMesh
    %   Brief: Main class for post processing operations
    %   Author: S.Ramon
    %   Version: 0.0.1
    
    properties
        filename
        time
    end
    
    methods
        function object = CPostProcessing(filename, time)
            if nargin > 0
                object.filename = filename;
                object.time = time;
            end
        end
        toGID( pn, xpoints, T, nnodepelem, nelem, nnode );
        toGIDPost( filename, time, nnodepelem, ndofpn, ngaus, disp );
        gidWriteVField( fid,nameres,time,vfield );
        gidWriteHeaderPost( fid,gtype,ngaus,job );
    end
    
end

